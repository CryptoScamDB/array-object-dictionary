{
  "name": "@cryptoscamdb/array-object-dictionary",
  "version": "1.1.0",
  "description": "A simple NPM package to map an array of objects to an object of unique values",
  "main": "lib/array-object-dictionary.js",
  "typings": "typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CryptoScamDB/array-object-dictionary.git"
  },
  "keywords": [
    "array",
    "object",
    "dictionary",
    "reverse",
    "unique"
  ],
  "author": "Luit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CryptoScamDB/array-object-dictionary/issues"
  },
  "homepage": "https://github.com/CryptoScamDB/array-object-dictionary#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@types/node": "^10.11.3",
    "@types/webpack": "^4.4.13",
    "@types/webpack-node-externals": "^1.6.3",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "tscheck": "tsc --noEmit --project tsconfig.json",
    "tslint": "tslint --project .",
    "prepublishOnly": "npm run clean && npm run build:prod",
    "clean": "rimraf ./lib",
    "build": "webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "prettier:diff": "prettier --write --config ./.prettierrc --list-different 'src/**/*.ts'"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --config ./.prettierrc --config-precedence file-override",
      "tslint --project .",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged",
      "pre-push": "npm run tslint && npm run tscheck"
    }
  }
}
